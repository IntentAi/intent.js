{
  "name": "intent.js",
  "version": "0.1.0",
  "description": "JavaScript/TypeScript bot SDK for Intent (discord.js compatible)",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "test": "echo 'Tests coming soon'",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "intent",
    "bot",
    "discord",
    "chat"
  ],
  "author": "IntentAi",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/IntentAi/intent.js.git"
  },
  "dependencies": {
    "ws": "^8.16.0",
    "@msgpack/msgpack": "^3.0.0-beta2",
    "undici": "^6.6.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
